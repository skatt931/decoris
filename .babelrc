{
	"presets": [
		"@babel/typescript",
		["@babel/env", {
			"targets": "last 2 version",
			"modules": false
		}]
		,"@babel/preset-react"
	],
	"plugins": [
		"@babel/proposal-class-properties",
		"@babel/proposal-object-rest-spread",
        "babel-plugin-styled-components",
		["@babel/plugin-transform-typescript", {
			"allowNamespaces": true
		}]
	]
}